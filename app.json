{
  "expo": {
    "name": "Mood Quotes",
    "slug": "MoodQuotes",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/Open.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.diptiverma.MoodQuotes"
    },
    "web": {
      "favicon": "./assets/images.png"
    }
  }
}
